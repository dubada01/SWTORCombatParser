<UserControl x:Class="SWTORCombatParser.Views.Battle_Review.MapView"
             xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:SWTORCombatParser.Views.Battle_Review"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <Image Source="{Binding BossMapImagePath}"></Image>
        <ContentPresenter Content="{Binding WPFPlot}"></ContentPresenter> 
    </Grid>
</UserControl>
