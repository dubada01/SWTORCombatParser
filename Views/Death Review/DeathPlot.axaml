<UserControl x:Class="SWTORCombatParser.Views.Death_Review.DeathPlot"
             xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:SWTORCombatParser.Views.Death_Review"
                xmlns:vm="clr-namespace:SWTORCombatParser.ViewModels.Death_Review"
                xmlns:avalonia="clr-namespace:ScottPlot.Avalonia;assembly=ScottPlot.Avalonia"
                x:DataType="vm:DeathPlotViewModel"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid Background="{StaticResource Gray3Brush}">
        <avalonia:AvaPlot Name="PlotArea" PointerPressed="GridView_MouseDown" PointerReleased="GridView_MouseUp" PointerMoved="GridView_MouseMove"/>
    </Grid>
</UserControl>
